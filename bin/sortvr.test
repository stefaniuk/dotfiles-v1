#!/bin/bash

file1=~/tmp/test_sortvr_1.$$
file2=~/tmp/test_sortvr_2.$$
file3=~/tmp/test_sortvr_3.$$

cat <<EOF > $file1
1.2.100.4
1.2.3.4
10.1.2.3
9.1.2.3
EOF
cat <<EOF > $file2
10.1.2.3
9.1.2.3
1.2.100.4
1.2.3.4
EOF

cat $file1 | sortvr > $file3
cmp --silent $file2 $file3
assert_result_pos $?

rm -f ~/tmp/test_sortvr_*
