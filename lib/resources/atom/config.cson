"*":
  autosave:
    enabled: true
  "command-palette":
    preserveLastSearch: true
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "welcome"
    ]
    excludeVcsIgnoredPaths: false
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: false
    fontSize: 16
    showInvisibles: true
    softWrap: true
    tabLength: 4
  "file-icons":
    onChanges: true
  "git-diff":
    showIconsInEditorGutter: true
  "highlight-selected":
    lightTheme: true
  "linter-ui-default":
    showPanel: true
  minimap:
    autoToggle: false
  "tree-view":
    hideVcsIgnoredFiles: false
