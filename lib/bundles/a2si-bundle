#!/bin/bash

# ### Mac OS X #################################################################

function macosx-install-a2si-bundle {

    run_install \
        \
        dependencies-bundle \
        system-bundle \
        admin-bundle \
        developer-bundle \
        \
        phpstorm \
        intellij \
        slack \
        vmware-horizon-client
}

function macosx-config-a2si-bundle {

    run_config \
        dependencies-bundle \
        system-bundle \
        admin-bundle \
        developer-bundle
}
